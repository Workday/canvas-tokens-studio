{
  "input": {
    "container": {
      "default": {
        "value": {
          "borderWidth": "1",
          "borderRadius": "{shape.x1}",
          "borderColor": "{color.border.input.default}",
          "fill": "{color.bg.default}",
          "boxShadow": "none",
          "itemSpacing": "{space.x2}"
        },
        "type": "composition"
      },
      "hover": {
        "value": {
          "borderWidth": "1",
          "borderRadius": "{shape.x1}",
          "borderColor": "{color.border.input.hover}",
          "fill": "{color.bg.default}",
          "boxShadow": "none",
          "itemSpacing": "{space.x2}"
        },
        "type": "composition"
      },
      "focus": {
        "value": {
          "borderWidth": "1",
          "borderRadius": "{shape.x1}",
          "borderColor": "{brand.common.focus-outline}",
          "fill": "{color.bg.default}",
          "boxShadow": "{input.focus.inactive}",
          "itemSpacing": "{space.x2}"
        },
        "type": "composition"
      },
      "alert": {
        "value": {
          "borderWidth": "1",
          "borderRadius": "{shape.x1}",
          "borderColor": "{brand.alert.darkest}",
          "fill": "{color.bg.default}",
          "boxShadow": "{input.focus.alert}",
          "itemSpacing": "{space.x2}"
        },
        "type": "composition"
      },
      "error": {
        "value": {
          "borderWidth": "1",
          "borderRadius": "{shape.x1}",
          "borderColor": "{brand.common.error-inner}",
          "fill": "{color.bg.default}",
          "boxShadow": "{input.focus.error}",
          "itemSpacing": "{space.x2}"
        },
        "type": "composition"
      }
    },
    "focus": {
      "outer": {
        "ring": {
          "value": [
            {
              "x": "0",
              "y": "0",
              "blur": "0",
              "spread": "2",
              "color": "{palette.french-vanilla.100}",
              "type": "dropShadow"
            },
            {
              "x": "0",
              "y": "0",
              "blur": "0",
              "spread": "4",
              "color": "{brand.common.focus-outline}",
              "type": "dropShadow"
            }
          ],
          "type": "boxShadow"
        }
      },
      "inactive": {
        "value": [
          {
            "x": "0",
            "y": "0",
            "blur": "0",
            "spread": "2",
            "color": "{brand.common.focus-outline}",
            "type": "innerShadow"
          }
        ],
        "type": "boxShadow"
      },
      "error": {
        "value": [
          {
            "x": "0",
            "y": "0",
            "blur": "0",
            "spread": "2",
            "color": "{brand.common.error-inner}",
            "type": "innerShadow"
          }
        ],
        "type": "boxShadow"
      },
      "alert": {
        "value": [
          {
            "x": "0",
            "y": "0",
            "blur": "0",
            "spread": "3",
            "color": "{brand.common.alert-inner}",
            "type": "innerShadow"
          }
        ],
        "type": "boxShadow"
      }
    },
    "label": {
      "value": {
        "typography": "{type.subtext.large}",
        "fill": "{color.fg.type.body}",
        "fontWeights": "{font-weight.500}"
      },
      "type": "composition",
      "description": "\n"
    },
    "text": {
      "value": {
        "typography": "{type.body.small}",
        "fill": "{color.fg.type.body}"
      },
      "type": "composition",
      "description": "\n"
    },
    "message": {
      "default": {
        "value": {
          "typography": "{type.subtext.medium}",
          "fill": "{color.fg.type.hint}"
        },
        "type": "composition",
        "description": "\n"
      },
      "alert": {
        "value": {
          "typography": "{type.subtext.medium}",
          "fill": "{color.fg.type.body}"
        },
        "type": "composition",
        "description": "\n"
      },
      "error": {
        "value": {
          "typography": "{type.subtext.medium}",
          "fill": "{color.fg.type.error}"
        },
        "type": "composition",
        "description": "\n"
      }
    },
    "gap": {
      "value": {
        "itemSpacing": "{space.x1}"
      },
      "type": "composition"
    }
  }
}